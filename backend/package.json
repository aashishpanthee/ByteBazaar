{
  "name": "ByteBazaar-backend",
  "version": "1.0.0",
  "description": "Ecommerce application built with MERN stack",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix frontend",
